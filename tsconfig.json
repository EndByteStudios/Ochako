{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "rootDir": "./src",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "baseUrl": "./src",
        "paths": {
            "@utils/*": ["utils/*"],
            "@commands/*": ["commands/*"],
            "@models/*": ["models/*"],
            "@types/*": ["types/*"],
            "@/*": ["./*"]
        }
    }
}
